{
  "rewrites": [
    { "source": "/webservices/:path*", "destination": "https://www.guicheweb.com.br/webservices/:path*" },
    { "source": "/((?!.*\\.html$).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
